{
  "kind": "collectionType",
  "collectionName": "vweblogs",
  "info": {
    "singularName": "vweblog",
    "pluralName": "vweblogs",
    "displayName": "vweblog",
    "description": ""
  },
  "options": {
    "draftAndPublish": true
  },
  "attributes": {
    "title": {
      "type": "string"
    },
    "content": {
      "type": "blocks"
    },
    "image": {
      "type": "media",
      "multiple": false,
      "required": false,
      "allowedTypes": [
        "images",
        "files",
        "videos",
        "audios"
      ]
    },
    "slug": {
      "type": "uid",
      "targetField": "title"
    },
    "categories": {
      "type": "relation",
      "relation": "manyToMany",
      "target": "api::category.category",
      "inversedBy": "vweblogs"
    },
    "readTime": {
      "type": "integer"
    },
    "links": {
      "type": "component",
      "repeatable": false,
      "component": "twitter.social-links"
    },
    "views": {
      "type": "integer",
      "default": 0
    }
  }
}
